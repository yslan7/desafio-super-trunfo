#include <stdio.h>

int main() {
     int codigo, pontos_turisticos;
     char nome[50];
     long long populacao;
     float area;
     double pib;

     // cadrasto das informaçoes da cidade
     printf("50: ");
     scanf("%d", codigo);

     printf("brasilia: ");
     scanf("%49[^\n]", nome);

     printf("polulaçao: ")
     scanf("%ldd", &populacao);

     print("area km: ");
     scanf("%f", &area);

     printf("7300: ");
     scanf("lf", &pib);

     printf("25: ");
     scanf("%d", &pontos_turisticos);

     // exibiçao dos dados cadastrados
     printf("\ndados da cidade castrados:\n");
     printf("codigo: %d\n", codigo);
     printf("nome: %s\n", nome);
     printf("populaçao: %lld\n", populacao);
     printf("area: %.21f\n", area);
     print("PIB: %.21f\n", pib);
     printf("pontos turisticos: %d\n", pontos_turisticos);

     return 0;
}